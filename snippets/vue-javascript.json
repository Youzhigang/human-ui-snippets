{
  "human import component from suites": {
    "prefix": "impst",
    "body": [
      "import ${1:suite} from 'vue-human/suites/${1:suite}'",
      "$2"
    ],
    "description": "import human component from suites"
  },
  "human import fn from utils": {
    "prefix": "imputl",
    "body": [
      "import ${1:Function} from 'vue-human/utils/${1:Function}'",
      "$2"
    ],
    "description": "import human function from utils"
  },
  "human import icons": {
    "prefix": "impicon",
    "body": [
      "import ${1:icon} from 'vue-human-icons/js/${2:ios}/${1:icon}'",
      "$3"
    ]
  },
  "human import input" : {
    "prefix": "impinput",
    "body": [
      "import input from 'vue-human/suites/input'",
      "$1"
    ],
    "description": "import vue-human input"
  },
  "human import search bar": {
    "prefix": "impsearch",
    "body": [
      "import searchBar from 'vue-human/suites/searchBar'",
      "$1"
    ],
    "description": "import vue-human search bar"
  },
  "human import button" : {
    "prefix": "impbtn",
    "body": [
      "import button from 'vue-human/suites/button'",
      "$1"
    ],
    "description": "import vue-human button"
  },
  "human import textarea" : {
    "prefix": "imptextarea",
    "body": [
      "import textarea from 'vue-human/suites/textarea'",
      "$1"
    ],
    "description": "import vue-human textarea"
  },
  "human import assistivebar": {
    "prefix": "impassistivebar",
    "body": [
      "import assistiveBar from 'vue-human/suites/assistiveBar'",
      "$1"
    ],
    "description": "import human assistivebar"
  },
  "human import actionsheet": {
    "prefix": "impactionsheet",
    "body": [
      "import actionSheet from 'vue-human/suites/actionSheet'",
      "$1"
    ],
    "description": "import human actionSheet"
  },
  "human import alert": {
    "prefix": "impalert",
    "body": [
      "import Alert from 'vue-human/utils/Alert'",
      "$1"
    ],
    "description": "import human alert"
  },
  "human alert show": {
    "prefix": "mnalertshow",
    "body": [
      "Alert.create({",
      "\ttitle: {$1:'警告'}",
      "}).show()"
    ],
    "description": "import human alert"
  },
  "human import message": {
    "prefix": "impmessage",
    "body": [
      "import Message from 'vue-human/utils/Message'",
      "$1"
    ],
    "description": "import human message"
  },
  "human import cell": {
    "prefix": "impcell",
    "body": [
      "import cell from 'vue-human/suites/cell'",
      "$1"
    ],
    "description": "import human cell"
  },
  "human message show": {
    "prefix": "mnmsgshow",
    "body": [
      "Message.create({",
      "\tmessage:'${1:message}',",
      "\ttheme:'${2:primary}',",
      "\tduration: 3000",
      "}).show()"
    ],
    "description": "import human message"
  },
  "human carousel": {
    "prefix": "impcarousel",
    "body":[
      "import carousel from 'vue-human/suites/carousel'",
      "$1"
    ],
    "description": "import human carousel"
  },
  "human check": {
    "prefix": "impcheck",
    "body":[
      "import carousel from 'vue-human/suites/check'",
      "$1"
    ],
    "description": "import human check"
  },
  "human confirm": {
    "prefix": "impconfirm",
    "body":[
      "import Confirm from 'vue-human/utils/Confirm'",
      "$1"
    ],
    "description": "import human confirm"
  },
  "human confirm show": {
    "prefix": "mnconfirmshow",
    "body": [
      "Confirm.create({$1}).show()"
    ],
    "description": "human confirm show"
  },
  "human import counter": {
    "prefix": "impcounter",
    "body": [
      "import counter from 'vue-human/suites/counter'",
      "$1"
    ],
    "description": "import human counter"
  },
  "human datetime layer show": {
    "prefix": "mndateshow",
    "body": [
      "Datetime.create({",
      "\tdefault: ${1:this.models.time},",
      "\tshowDate: ${2:false}",
      "}).show().on('confirm', time => {",
      "\t${3:this.models.time} = time",
      "})"
    ]
  },
  "human loadingMask show": {
    "prefix": "mnloadmaskshow",
    "body":[
      "LoadingMask.create().show()"
    ],
    "description": "vue-human loading show"
  }
}